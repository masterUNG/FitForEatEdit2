<?xml version="1.0" encoding="utf-8"?>


    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/b8">


        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="match_parent"
                    android:layout_height="93dp"
                    app:srcCompat="@drawable/chest1"
                    android:layout_marginTop="0dp"
                    tools:layout_editor_absoluteX="0dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="match_parent"
                    android:layout_height="93dp"
                    app:srcCompat="@drawable/arm1"
                    tools:layout_editor_absoluteX="0dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="match_parent"
                    android:layout_height="93dp"
                    app:srcCompat="@drawable/shoulder1"
                    tools:layout_editor_absoluteX="0dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="match_parent"
                    android:layout_height="93dp"
                    app:srcCompat="@drawable/l1"
                    tools:layout_editor_absoluteX="0dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="match_parent"
                    android:layout_height="93dp"
                    app:srcCompat="@drawable/abs1"
                    tools:layout_editor_absoluteX="0dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="match_parent"
                    android:layout_height="93dp"
                    app:srcCompat="@drawable/back1"
                    tools:layout_editor_absoluteX="0dp"
                    tools:layout_editor_absoluteY="0dp" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="48dp"
                    />






            </LinearLayout>
        </ScrollView>


    <TableRow
            android:id="@+id/tableRow"
            android:layout_width="0dp"
            android:layout_height="59dp"
            android:background="@drawable/t2"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="73dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/h1"
                tools:layout_editor_absoluteX="306dp"
                tools:layout_editor_absoluteY="445dp" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="70dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/f1"
                tools:layout_editor_absoluteX="264dp"
                tools:layout_editor_absoluteY="455dp" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="84dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/d1"
                tools:layout_editor_absoluteX="101dp"
                tools:layout_editor_absoluteY="455dp" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/information1" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="71dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/x1"
                tools:layout_editor_absoluteX="233dp"
                tools:layout_editor_absoluteY="443dp" />

        </TableRow>


</android.support.constraint.ConstraintLayout>

